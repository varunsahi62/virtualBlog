
<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <br>
    <input class="form-control" type="text" autocomplete="off" name="postTitle" placeholder=" Blog Title...">
    <br>
    <textarea class="form-control" name="postBody" rows="5" cols="30" placeholder=" Blog Content..."></textarea>
    <br>
  </div>
  <button class="btn btn-primary" style="font-family: monospace;" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer"); -%>
